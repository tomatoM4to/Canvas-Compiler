{
  "manifest_version": 3,
  "name": "웹스톰",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "service_worker": "app/background.js"
  },
  "permissions": [
    "tabs", "webNavigation", "scripting", "activeTab"
  ]
}
